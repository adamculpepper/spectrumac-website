---
layout: layouts/base.njk
section: services
---

{{ layoutContent | safe }}


<h2>services</h2>
<ul>
{%- for service in collections.services -%}
  <li>{{ service.data.title }} - {{ service.url }} - {{ service.data.body }}</li>
{%- endfor -%}
</ul>


<h2>products</h2>
<ul>
{%- for product in collections.products -%}
  <li>{{ product.data.title }} - {{ product.url }} - {{ product.data.body }}</li>
{%- endfor -%}
</ul>


